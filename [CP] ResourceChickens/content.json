			"Section": "MayoMachine"
		},
		"DeluxeMayoMinutesUntilReady": {
			"Default": 180,
			"Section": "MayoMachine"
		}
	},
	// Dynamic Tokens
	"DynamicTokens": [
		{
			"Name": "ResourceEggConverterCraftingRecipe",
			"Value": "{{ConverterCraftingIngredient1}} {{ConverterCraftingIngredient1Amount}} {{ConverterCraftingIngredient2}} {{ConverterCraftingIngredient2Amount}} {{ConverterCraftingIngredient3}} {{ConverterCraftingIngredient3Amount}} {{ConverterCraftingIngredient4}} {{ConverterCraftingIngredient4Amount}}"
		},
		{
			"Name": "CopperEggFusionChamberRecipe",
			"Value": "{{ModId}}.LargeStoneEgg 1 {{ModId}}.LargeCoalEgg 1 {{CopperChamberCraftingIngredient3}} {{CopperChamberCraftingIngredient3Amount}} {{CopperChamberCraftingIngredient4}} {{CopperChamberCraftingIngredient4Amount}}"
		},
		{
			"Name": "HardwoodEggFusionChamberRecipe",
			"Value": "{{ModId}}.LargeWoodEgg 1 {{ModId}}.LargeClayEgg 1 {{HardwoodChamberCraftingIngredient3}} {{HardwoodChamberCraftingIngredient3Amount}} {{HardwoodChamberCraftingIngredient4}} {{HardwoodChamberCraftingIngredient4Amount}}"
		}
	],
	// Changes
	"Changes": [
		// Machine and Eggs
		{
			"Action": "Load",
			"Target": "{{ModId}}/Eggs, {{ModId}}/ResourceEggConverter",
			"FromFile": "assets/{{TargetWithoutPath}}.png"
		},
		// Clay Chicken
		{
			"Action": "Load",
			"Target": "{{ModId}}/ClayChicken, {{ModId}}/ClayChickenBaby, {{ModId}}/ClayShopTexture,",
			"FromFile": "assets/{{TargetWithoutPath}}.png"
		},
		// Coal Chicken
		{
			"Action": "Load",
			"Target": "{{ModId}}/CoalChicken, {{ModId}}/CoalChickenBaby, {{ModId}}/CoalShopTexture,",
			"FromFile": "assets/{{TargetWithoutPath}}.png"
		},
		// Copper Chicken
		{
			"Action": "Load",
			"Target": "{{ModId}}/CopperChicken, {{ModId}}/CopperChickenBaby",
			"FromFile": "assets/{{TargetWithoutPath}}.png"
		},
		// Fiber Chicken
		{
			"Action": "Load",
			"Target": "{{ModId}}/FiberChicken, {{ModId}}/FiberChickenBaby",
			"FromFile": "assets/{{TargetWithoutPath}}.png"
		},
		// Hardwood Chicken
		{
			"Action": "Load",
			"Target": "{{ModId}}/HardwoodChicken, {{ModId}}/HardwoodChickenBaby",
			"FromFile": "assets/{{TargetWithoutPath}}.png"
		},
		// Stone Chicken
		{
			"Action": "Load",
			"Target": "{{ModId}}/StoneChicken, {{ModId}}/StoneChickenBaby, {{ModId}}/StoneShopTexture,",
			"FromFile": "assets/{{TargetWithoutPath}}.png"
		},
		// Wood Chicken
		{
			"Action": "Load",
			"Target": "{{ModId}}/WoodChicken, {{ModId}}/WoodChickenBaby, {{ModId}}/WoodShopTexture,",
			"FromFile": "assets/{{TargetWithoutPath}}.png"
		},
		// Include Chickens
		{
			"Action": "Include",
			"FromFile": "chickens/clay.json, chickens/coal.json, chickens/copper.json, chickens/fiber.json, chickens/hardwood.json, chickens/stone.json, chickens/vanilla.json, chickens/wood.json"
		},
		// Include Other Data
		{
			"Action": "Include",
			"FromFile": "data/craftingrecipes.json, data/machines.json"
		}
	]
}

