					"EggItemIds": ["{{ModId}}.MossEgg", "{{ModId}}.LargeMossEgg", "{{ModId}}.MossEggFusionChamber"],
					"IncubationTime": "{{Round: {{query: {{ResourceChickenIncubationTimeChoice}} * 1}}, 0, down }}",
					"BirthText": "{{i18n: Chicken.MossChicken.BirthText}}",
