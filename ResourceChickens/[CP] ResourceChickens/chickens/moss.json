{
	"Changes": [
		// Moss Chicken
		{
			"Action": "EditData",
			"Target": "Data/FarmAnimals",
			"Entries": {
				"{{ModId}}.MossChicken": {
					"DisplayName": "{{i18n: Chicken.MossChicken.Name}}",
